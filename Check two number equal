#method 1
n1=int(input("enter a number n1:"))
n2=int(input("enter a number n2:"))
if n1==n2:
   print ("equal")
else:
  print ("inputed values for n1 and n2 are different")
#method 2
n1=int(input("enter a number n1:"))
n2=int(input("enter a number n2:"))
if n1%n2==0:
   print("equal")
else:
   print ("inputed values for n1 and n2 are different")

   
